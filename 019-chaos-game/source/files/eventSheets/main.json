									"conditions": [
										{
											"id": "compare-instance-variable",
											"objectClass": "Button",
											"sid": 528385312404083,
											"parameters": {
												"instance-variable": "Command",
												"comparison": 0,
												"value": "\"Subtract Points\""
											}
										}
									],
									"actions": [
										{
											"type": "script",
											"script": "const p = runtime.globalVars.PointsCount;\n\nconst quantity = p - Utils.match(p)\n     \t\t\t\t.on(p => p <= 1, () => 0)\n     \t\t\t\t.on(p => p > 1 & p <=10 , () => 1)\n\t\t\t\t\t.on(p => p > 10 & p <=100 , () => 10)\n\t\t\t\t\t.on(p => p > 100 & p <=1000 , () => 100)\n\t\t\t\t\t.on(p => p > 1000 & p <=10000 , () => 1000)\n\t\t\t\t\t.on(p => p > 10000 & p <=1000000 , () => 10000)\n     \t\t\t\t.otherwise(p => () => p*0);\n\t \nruntime.globalVars.PointsCount = quantity;\n\nconst points = runtime.objects.Points.getAllInstances();\nconst vertex = runtime.objects.Vertex.getAllInstances();\nconst show = true;\n\nGame.changeQuantityPoints({quantity, points, vertex, show})"
										}
									],
